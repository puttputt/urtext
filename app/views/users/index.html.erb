<h1>Welcome to your Profile!</h1>
<%= link_to "Messages", messages_path %>
<h2>Your Bookmarks</h2>
	<%- for bookmark in @bookmarks -%>
		<%= render :partial => "bookmarks", :locals => { :bmark => bookmark } %>
	<%- end -%>
<h2>Your Ads</h2>
	<%- for ad in @ads -%>
		<%= render :partial => "ads", :locals => {:ad => ad }%>
	<%- end -%>
	